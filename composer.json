{
    "name": "paulhenri-l/eloquent-builder-decorator",
    "description": "Allows you to decorate Laravel query builders",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "paulhenri-l",
            "email": "25308170+paulhenri-l@users.noreply.github.com"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "illuminate/database": "^9.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "phpunit/phpunit": "^9.5.10",
        "symfony/var-dumper": "^6.0",
        "symfony/finder": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "PaulhenriL\\EloquentBuilderDecorator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PaulhenriL\\EloquentBuilderDecorator\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "@php vendor/bin/phpunit"
    }
}